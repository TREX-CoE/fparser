nblk_max 	  200
nopt_iter   2

sr_tau 		  1.023
sr_eps 		  0.001
sr_adiag 	  0.05
energy_tol 	0.004

%block optimization_flags
   optimize_wavefunction 		no		
   optimize_ci 		 		      No
